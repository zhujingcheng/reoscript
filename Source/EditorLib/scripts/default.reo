// object 'script' is the global object like 'window' in JavaScript
//

function myfunc() {
  return 'hello world';
}

script.add = function(a, b) {
  return a + b;
};

var arr = [1, 5, 10, 20, 24, 27, 31, 45, 69, 82];

Array.prototype.sum = () => { 
  var total = 0;
  for(element in this) total += element;
  return total;
};

console.log(myfunc() + ' [' + add(10, arr.sum()) + ']');
